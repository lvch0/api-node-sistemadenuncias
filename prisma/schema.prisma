generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usuario {
  IdUsuario     Int    @id @default(autoincrement())
  nombreUsuario String @db.VarChar(30)
  contrasenia   String @db.VarChar(8)
  rol           String @db.VarChar(30)
}
